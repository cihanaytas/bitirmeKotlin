<?xml version="1.0" encoding="utf-8"?>
<layout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto">


<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/commentConst"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.customer.CustomerNotificationsFragment">


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/CustomerNotificationsListRecyclerView"
        android:layout_width="410dp"
        android:layout_height="0dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/noNotificationMessage2"
        android:layout_width="269dp"
        android:layout_height="109dp"
        android:layout_marginStart="68dp"
        android:layout_marginBottom="304dp"
        android:text="Bildirim Bulunmuyor"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>

</layout>